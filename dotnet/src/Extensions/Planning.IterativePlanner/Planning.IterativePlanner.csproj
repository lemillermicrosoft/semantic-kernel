    <Compile Include="..\..\InternalUtilities\Diagnostics\Verify.cs" Link="Diagnostics\Verify.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="skprompt.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
