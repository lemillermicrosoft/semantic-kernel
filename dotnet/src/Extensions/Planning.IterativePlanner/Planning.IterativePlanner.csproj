    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="skprompt.txt" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\..\InternalUtilities\Diagnostics\CompilerServicesAttributes.cs" Link="Diagnostics\CompilerServicesAttributes.cs" />
